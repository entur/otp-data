            "crossCountryRail",
            "railReplacementBus"
          ]
        }, {
          "agencies": [
            "RB:ATU:Authority:1",
            "RB:BOR:Authority:1",
            "RB:BSR:Authority:1",
            "RB:BSR:Authority:2",
            "RB:TID:Authority:TID",
            "RB:UNI:Authority:UNI"
          ]
        }, {
          "agencies": [
            "RB:VYB:Authority:1",
            "RB:VYX:Authority:1"
          ]
        }, {
          "agencies": [
            "RB:FLI:Authority:FlixMobility"
          ]
        }, {
          "agencies": [
            "RB:NBU:Authority:002"
          ]
        }, {
          "agencies": [
            "RB:NWY:Authority:d1ea7af1-3c2a-4825-893f-21408ef08480"
          ]
        }
      ]
    }
  },
  "transit": {
    "maxNumberOfTransfers": 12,
    "maxSearchWindow": "48h",
    "dynamicSearchWindow": {
      "minTransitTimeCoefficient": 0.5,
      "minWaitTimeCoefficient": 0.5,
      "minWindow": "1h",
      "maxWindow" : "5h"
    },
    "stopBoardAlightDuringTransferCost": {
      "DISCOURAGED": 1500,
      "ALLOWED": 75,
      "RECOMMENDED": 30,
      "PREFERRED" : 0
    }
  },
  "flex": {
    "maxTransferDuration": "5m",
    "maxFlexTripDuration": "1h15m"
